{
    "builds": [
        {
            "src": "portal/**",
            "use": "@vercel/static"
        },
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/portal/$1"
        }
    ]
}